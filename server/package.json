{
  "name": "opentx-logbook-server",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "Flight logbook from OpenTX logs",
  "main": "server.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "build": "tsc -p ./src",
    "start:prod": "NODE_ENV=production node dist/server/src/server.js",
    "start": "tsc-watch -p ./src --onSuccess \"node dist/server/src/server.js\""
  },
  "keywords": [
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skarppi/logbook.git"
  },
  "license": "MIT",
  "author": "Juho Kolehmainen",
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^5.0.0-beta.1",
    "body-parser": "^1.18.3",
    "csv-parse": "^4.3.0",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "graphql-request": "^1.8.2",
    "http-proxy-middleware": "^0.18.0",
    "multer": "^1.4.1",
    "pg-promise": "^8.5.4",
    "postgraphile": "^4.3.3",
    "postgraphile-plugin-connection-filter": "^1.0.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "tsc-watch": "^1.0.31",
    "tslint": "^5.12.0",
    "typescript": "^3.7.5"
  }
}
