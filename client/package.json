{
  "name": "opentx-logbook-client",
  "description": "Flight logbook from OpenTX logs",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "build": "cd ./src && NODE_ENV=production webpack -p",
    "start": "cd ./src && webpack-dev-server -w",
    "profile": "cd ./src && NODE_ENV=production webpack -p --json --profile > /tmp/webpack-build-log.json",
    "analyze": "webpack-bundle-analyzer /tmp/webpack-build-log.json dist/public/"
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skarppi/logbook.git"
  },
  "license": "MIT",
  "author": "Juho Kolehmainen",
  "dependencies": {
    "@material-ui/core": "^3.9.4",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
    "date-fns": "^2.9.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^8.0.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "urql": "^1.9.0",
    "video-react": "^0.14.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "open-browser-webpack-plugin": "0.0.5",
    "style-loader": "^0.21.0",
    "tsc-watch": "^1.0.31",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
