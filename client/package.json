{
  "name": "opentx-logbook-client",
  "description": "Flight logbook from OpenTX logs",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "build": "cd ./src && NODE_ENV=production webpack -p",
    "start": "cd ./src && webpack-dev-server -w",
    "profile": "cd ./src && NODE_ENV=production webpack -p --json --profile > /tmp/webpack-build-log.json",
    "analyze": "webpack-bundle-analyzer /tmp/webpack-build-log.json dist/public/"
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skarppi/logbook.git"
  },
  "license": "MIT",
  "author": "Juho Kolehmainen",
  "dependencies": {
    "@material-ui/core": "^3.8.3",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "chart.js": "2.5.0",
    "date-fns": "^1.30.1",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "http-proxy-middleware": "^0.18.0",
    "jss": "^9.8.7",
    "lodash": "^4.17.11",
    "multer": "^1.4.1",
    "react": "^16.7.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.7.0",
    "react-dropzone": "^8.0.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "tslib": "^1.9.3",
    "urql": "^1.0.4",
    "video-react": "^0.13.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "open-browser-webpack-plugin": "0.0.5",
    "style-loader": "^0.21.0",
    "tsc-watch": "^1.0.31",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}