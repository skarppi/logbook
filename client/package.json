{
  "name": "opentx-logbook-client",
  "description": "Flight logbook from OpenTX logs",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "build": "cd ./src && NODE_ENV=production webpack -p",
    "start": "cd ./src && webpack-dev-server -w",
    "profile": "cd ./src && NODE_ENV=production webpack -p --json --profile > /tmp/webpack-build-log.json",
    "analyze": "webpack-bundle-analyzer /tmp/webpack-build-log.json dist/public/"
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skarppi/logbook.git"
  },
  "license": "MIT",
  "author": "Juho Kolehmainen",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "axios": "^0.21.1",
    "chart.js": "^2.9.3",
    "date-fns": "^2.16.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "leaflet": "^1.7.1",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.3",
    "react-hotkeys-hook": "^2.3.0",
    "react-leaflet": "^2.7.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "urql": "^1.10.0",
    "video-react": "^0.14.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "awesome-typescript-loader": "^5.2.1",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^4.2.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "open-browser-webpack-plugin": "0.0.5",
    "style-loader": "^1.2.1",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
