{
  "name": "opentx-logbook",
  "version": "1.0.0",
  "description": "Flight logbook from OpenTX logs",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "node ./dist/server/server.js",
    "start:prod": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "build-client": "cd ./src/client && cross-env NODE_ENV=production webpack -p",
    "dev-client": "cd ./src/client && webpack-dev-server -w",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node ./dist/server/server.js\"",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\""
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skarppi/opentx-logbook.git"
  },
  "license": "MIT",
  "author": "Juho Kolehmainen",
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "csv-parse": "^4.3.0",
    "date-fns": "^1.30.1",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.18.0",
    "multer": "^1.4.1",
    "pg-promise": "^8.5.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-dropzone": "^8.0.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "tslib": "^1.9.3",
    "typesafe-actions": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/redux-logger": "^3.0.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "open-browser-webpack-plugin": "0.0.5",
    "redux-devtools-extension": "^2.13.7",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "tsc-watch": "^1.0.31",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
