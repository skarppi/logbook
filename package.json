{
  "name": "opentx-logbook",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "Flight logbook from OpenTX logs",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "start": "node ./server/dist/server/server.js",
    "start:prod": "NODE_ENV=production node ./dist/server/server/server.js",
    "dev": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"cd server && npm run dev\" \"cd client && npm run dev\"",
    "build": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"cd server && npm run build\" \"cd client && npm run build\""
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skarppi/logbook.git"
  },
  "license": "MIT",
  "author": "Juho Kolehmainen",
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^3.6.1",
    "tslint": "^5.12.0"
  }
}
